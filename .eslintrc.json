{  "parserOptions": {    "ecmaVersion": "latest",    "sourceType": "module"  },  "plugins": ["prettier"],  "extends": [    "eslint:recommended",    "plugin:prettier/recommended"  ],  "rules": {    "prettier/prettier": "error",    "import/no-extraneous-dependencies": "off",    "node/no-extraneous-import": "off",    "node/no-unpublished-import": "off",    "import/no-unresolved": "off",    "node/no-missing-import": "off",    "no-useless-escape": "off"  },  "env": {    "browser": true,    "es2022": true,    "node": true  }}